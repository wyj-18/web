<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <filter>
        <filter-name>Filter</filter-name>
        <filter-class>filter.Filter</filter-class>
        <init-param>
            <param-name>notFilterUrl</param-name>
            <param-value>/createVerifyImage.do,/ajaxLoginCheck.do,/quit.do,/checkExist.do,/provinceCity.do,/register.do</param-value>
        </init-param>
        <init-param>
            <param-name>mustFilterUrl</param-name>
            <param-value>/resourcesManage.do,/userManage.do,/search.do,/updateUser.do,/resourcesManage.jsp,/userManage.jsp</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>Filter</filter-name>
        <url-pattern>*.jsp</url-pattern>
        <url-pattern>*.do</url-pattern>
    </filter-mapping>

    <welcome-file-list>
        <welcome-file>login.html</welcome-file>
    </welcome-file-list>
</web-app>